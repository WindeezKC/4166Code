<head>
    <%- include('partials/header')  -%>
    <link href="/assests/stylesheets/styles.css" rel="stylesheet" type="text/css" />
    
    <meta charset="UTF-8">
    <title>Title</title>
</head>

<body>
    
    <div class="main">
<table id="course">
 
    <%if (typeof theCourse != 'undefined'){ %>

    <tr>
        <th>Course Number</th>
        <th>Title</th>
        <th>Term   </th>
        <th>Instructor</th>
    </tr>
    <tr>
        <td><%= theCourse.code %></td>
        <td><%= theCourse.title %></td>
        <td><%= theCourse.term %></td>
        <td><%= theCourse.instructor %></td>
    </tr>
   <tr>
       <td colspan ="4"></td> <%- include('image') -%> </td>
   </tr>
   <%} else {%>
    <tr>
        <th> <%= message %></th>
      </tr>
</table>

<%} %>



</body>

